<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


  <EditText
      android:id="@+id/edit_address"
      android:layout_width="325dp"
      android:layout_height="50dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_alignParentEnd="true"
      android:layout_marginTop="32dp"
      android:background="@drawable/custom_input"
      android:drawableEnd="@drawable/e4"
      android:drawablePadding="12dp"
      android:inputType="textPersonName"
      android:text=" Address"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.523"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <EditText
      android:id="@+id/edit_birthDate"
      android:layout_width="325dp"
      android:layout_height="50dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_alignParentEnd="true"
      android:layout_marginTop="92dp"
      android:background="@drawable/custom_input"
      android:drawableEnd="@drawable/e5"
      android:drawablePadding="12dp"
      android:inputType=""
      android:text=" Birthdate"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.529"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <EditText
      android:id="@+id/edit_umakEmail"
      android:layout_width="325dp"
      android:layout_height="50dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_alignParentEnd="true"
      android:layout_marginTop="152dp"
      android:background="@drawable/custom_input"
      android:drawableEnd="@drawable/e6"
      android:drawablePadding="12dp"
      android:inputType=""
      android:text=" UMAK Email"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.529"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <EditText
      android:id="@+id/edit_contactNumber"
      android:layout_width="325dp"
      android:layout_height="50dp"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_alignParentEnd="true"
      android:layout_marginTop="212dp"
      android:background="@drawable/custom_input"
      android:drawableEnd="@drawable/e7"
      android:drawablePadding="12dp"
      android:inputType=""
      android:text=" Contact Number"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.529"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView2" />

  <ImageView
      android:id="@+id/imageView2"
      android:layout_width="320dp"
      android:layout_height="319dp"
      android:layout_marginTop="52dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.496"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/register_image" />

  <Button
      android:id="@+id/BackRegisterPage2"
      android:layout_width="138dp"
      android:layout_height="48dp"
      android:layout_marginTop="36dp"
      android:background="#399CFF"
      android:text="Back"
      android:textSize="15dp"
      android:textStyle="bold"
      app:layout_constraintEnd_toStartOf="@+id/NextRegisterPage2"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/edit_contactNumber" />

  <Button
      android:id="@+id/NextRegisterPage2"
      android:layout_width="138dp"
      android:layout_height="48dp"
      android:layout_marginTop="36dp"
      android:background="#399CFF"
      android:text="Next"
      android:textSize="15dp"
      android:textStyle="bold"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.87"

      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/edit_contactNumber" />

</androidx.constraintlayout.widget.ConstraintLayout>