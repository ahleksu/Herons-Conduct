<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GoodMoralRequest_Admin">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="414dp"
        android:layout_height="64dp"
        android:background="@drawable/bluebar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/imageView29"
            android:layout_width="245dp"
            android:layout_height="40dp"
            android:layout_marginEnd="112dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/text21" />

        <Button
            android:id="@+id/button"
            android:layout_width="49dp"
            android:layout_height="43dp"
            android:layout_marginEnd="4dp"
            android:background="@drawable/blue_btn"
            android:drawableBottom="@drawable/back_btn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView29"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.69" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="329dp"
        android:layout_height="55dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/whitebox"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="214dp"
            android:layout_height="34dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="11dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/text22" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout4"
        android:layout_width="329dp"
        android:layout_height="389dp"
        android:layout_marginTop="-3dp"

        android:background="@drawable/whitebox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">


        <EditText
            android:id="@+id/goodMoralRequestAdmin"
            android:layout_width="281dp"
            android:layout_height="39dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/login_edit_text"
            android:elevation="10dp"
            android:hint=""
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.416"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView44" />

        <EditText
            android:id="@+id/goodMoralCollegeAdmin"
            android:layout_width="281dp"
            android:layout_height="39dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/login_edit_text"
            android:elevation="10dp"
            android:hint=""
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.395"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView46" />

        <EditText
            android:id="@+id/goodMoralDateReleaseAdmin"
            android:layout_width="281dp"
            android:layout_height="39dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/login_edit_text"
            android:drawableRight="@drawable/ic_baseline_calendar_month_24"
            android:elevation="10dp"
            android:ems="10"
            android:inputType="date"
            android:padding="10dp"
            android:paddingEnd="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.395"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView47" />

        <ImageView
            android:id="@+id/imageView44"
            android:layout_width="124dp"
            android:layout_height="20dp"
            android:layout_marginTop="36dp"
            android:background="@drawable/login_edit_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.098"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/text23" />

        <ImageView
            android:id="@+id/imageView45"
            android:layout_width="132dp"
            android:layout_height="16dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/login_edit_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.101"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/goodMoralRequestAdmin"
            app:srcCompat="@drawable/text24" />

        <EditText
            android:id="@+id/goodMoralEmailAdmin"
            android:layout_width="281dp"
            android:layout_height="39dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/login_edit_text"
            android:elevation="10dp"
            android:ems="10"
            android:inputType="textEmailAddress"
            android:padding="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.416"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView45" />

        <ImageView
            android:id="@+id/imageView46"
            android:layout_width="95dp"
            android:layout_height="20dp"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.081"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/goodMoralEmailAdmin"
            app:srcCompat="@drawable/text25" />

        <ImageView
            android:id="@+id/imageView47"
            android:layout_width="168dp"
            android:layout_height="22dp"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.124"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/goodMoralCollegeAdmin"
            app:srcCompat="@drawable/text26" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/button2"
        android:layout_width="128dp"
        android:layout_height="46dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/button2"
        android:text="Cancel"

        android:textColor="@color/white"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textAllCaps="false"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4" />

    <Button
        android:id="@+id/button3"
        android:layout_width="128dp"
        android:layout_height="46dp"

        android:layout_marginTop="44dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/button1"
        android:text="Notify"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout4" />

</androidx.constraintlayout.widget.ConstraintLayout>